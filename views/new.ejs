<%- include ./static/header -%>
	<form action = "#" method = 'post' >
	
	    <div class="form-group col-md-6">
	      <label for="socname">Society Name</label>
	      <input type="email" class="form-control" id="socname" placeholder="Society Name" name ="society[Name]">
	    </div>
  
	  <div class="form-group col-md-6">
	    <label for="inputAddress">Address</label>
	    <input type="text" class="form-control" id="inputAddress" placeholder="1234 Main St" name = "society[Addr]">
	  </div>
	  <div class="form-group col-md-6">
	    <label for="inputAddress2">Profile Picture Url</label>
	    <input type="text" class="form-control" id="inputAddress2" placeholder="Apartment, studio, or floor" name = "society[Profile_url]">
	  </div>
	    <div class="form-group col-md-6">
	      <label for="floors">Floors</label>
	      <input type="text" class="form-control" id="floors" name = "society[Floors]">
	    </div>
	    <div class="form-group col-md-6">
	      <label for="maxrooms">Maximum Rooms on floor currently </label>
	      <input type="text" class="form-control" id="maxrooms" name = "society[MaxRoomsOnFloor]">
	    </div>
	  <div class="form-group col-md-6">
	  	<button type="submit" class="btn btn-primary">Register Society</button>
	  </div>
	</form>
<%- include ./static/footer -%>