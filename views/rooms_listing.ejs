<%- include ./html/header -%>
		<%var floorNo = 0;%>
		<div id = 'accordion'>
		<% for( var floorName in Floors ){ %>
		<% var roomsOnFloor = Floors[ floorName ]; %>
			<div class = 'card'>
				<div class="card-header" id="headingOne">
					<h5 class="mb-0">
		        		<button class="btn btn-link" data-toggle="collapse" data-target="#collapse<%=floorNo%>" aria-expanded="false" aria-controls="collapseOne">
		          				<%=floorName[0].toUpperCase( ) + floorName.substr( 1 )%>
		        		</button>
		      		</h5>
				</div>
			</div>


			<div id="collapse<%=floorNo%>" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
  				<div class="card-body">
  					<div class = 'row'>
  					<% roomsOnFloor.forEach( function( room ){%>
  						<form action = "/room" class = 'col-md-4' method = 'post'>
  							<input type = 'hidden' id='room_id' name = 'room_id' value = "<%=room._id%>">
  							<button class = 'btn btn-md btn-success col-md-12' style = 'margin:5'><%=room.Room_No + 1 %></button>
  						</form>
  						
        			<%});%>
        			</div>
        		
      			</div>
    		</div>	
    		<%floorNo += 1%>
    	<%}%>
		</div>

<%- include ./html/footer -%>